{
    "name": "final-project",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^4.2.2",
        "@fortawesome/fontawesome-svg-core": "^1.2.32",
        "@fortawesome/free-regular-svg-icons": "^5.15.1",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.11",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "antd": "^4.8.0",
        "axios": "^0.20.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "formik": "^2.2.0",
        "node-sass": "^4.14.1",
        "npm-run-all": "^4.1.5",
        "npm-watch": "^0.7.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.13.6",
        "react": "^16.13.1",
        "react-burger-menu": "^2.8.0",
        "react-dom": "^16.13.1",
        "react-facebook-login": "^4.1.1",
        "react-google-login": "^5.1.22",
        "react-id-swiper": "^4.0.0",
        "react-redux": "^7.2.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "3.4.3",
        "react-slick": "^0.27.12",
        "react-test-renderer": "^16.13.1",
        "react-use": "^15.3.4",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-mock-store": "^1.5.4",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^2.3.0",
        "slick-carousel": "^1.8.1",
        "styled-components": "^5.2.0",
        "stylelint": "^13.7.2",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-order": "^4.1.0",
        "yup": "^0.29.3"
    },
    "scripts": {
        "start:dev": "npm-run-all -p watch-sass-js start-js",
        "start": "if-env NODE_ENV=production ?? npm run start:prod || npm run start:dev",
        "start:prod": "npm run build && react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "eslint": "eslint \"src/**/*.jsx\"",
        "start-js": "react-scripts start",
        "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
        "stylelint-build": "stylelint 'src/**/*.scss'",
        "watch-sass-js": "npm-watch"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "watch": {
        "stylelint-dev": {
            "patterns": [
                "/src/**/*.scss"
            ],
            "extensions": "scss"
        },
        "eslint": {
            "patterns": [
                "/src/**/*.jsx"
            ],
            "extensions": "jsx"
        }
    },
    "devDependencies": {
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.21.3",
        "standard": "^14.3.4"
    }
}
